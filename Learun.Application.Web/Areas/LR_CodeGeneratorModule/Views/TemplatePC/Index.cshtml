@{
    ViewBag.Title = "PC端代码生成器";
    Layout = "~/Views/Shared/_Index.cshtml";
}
@Html.AppendCssFile("/Areas/LR_CodeGeneratorModule/Views/TemplatePC/Index.css")
<div class="lr-layout">
    <div class="lr-layout-center">
        <div class="lr-layout-wrap">
            <div class="lr-cover-box">
                <div class="lr-cover-img">
                    <img class="lr-cover-img1" src="~/Content/images/codeGenerator/multitable0.png" />
                    <img class="lr-cover-img2" src="~/Content/images/codeGenerator/multitable1.png" />

                    <h4><span class="lrlg">自定义开发模板</span></h4>
                </div>
                <div class="lr-cover-info">
                    <small><span class="lrlg">通过设置生成单表或多表的增删改查功能。</span></small>
                </div>
                <div class="lr-cover-button">
                    <a class="btn btn-default" id="lr_custmerCode"><span class="lrlg">配置</span></a>
                </div>
            </div>
            <div class="lr-cover-box">
                <div class="lr-cover-img">
                    <img class="lr-cover-img1" src="~/Content/images/codeGenerator/singletable0.png" />
                    <img class="lr-cover-img2" src="~/Content/images/codeGenerator/singletable1.png" />

                    <h4><span class="lrlg">快速开发模板</span></h4>
                </div>
                <div class="lr-cover-info">
                    <small><span class="lrlg">快速生成必要的类和页面。</span></small>
                </div>
                <div class="lr-cover-button">
                    <a class="btn btn-default" id="lr_fastCode"><span class="lrlg">配置</span></a>
                </div>
            </div>
            <div class="lr-cover-box">
                <div class="lr-cover-img">
